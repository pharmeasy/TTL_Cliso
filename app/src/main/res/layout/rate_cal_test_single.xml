<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent_ll"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/underline_grey"
    android:orientation="horizontal"
    android:paddingLeft="10dp"
    android:paddingTop="10dp"
    android:paddingRight="25dp"
    android:paddingBottom="10dp"
    android:weightSum="5">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:layout_weight="4.9"
        android:orientation="vertical"
        android:paddingLeft="5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/test_name_rate_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:gravity="left|center"
                android:paddingTop="2dp"
                android:textAlignment="viewStart"
                android:textAllCaps="true"
                android:textColor="@color/grey"
                android:textSize="@dimen/value"
                tools:ignore="RtlCompat"
                tools:text="Aarogyam C" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MRP : "
                android:textColor="@color/grey"
                android:textSize="@dimen/note1" />

            <TextView
                android:id="@+id/test_rate_cal_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/grey"
                android:textSize="@dimen/note1"
                tools:text="₹ 1100/-" />

            <LinearLayout
                android:id="@+id/ll_your_rate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Your Rate : "
                    android:textColor="@color/grey"
                    android:textSize="@dimen/note1" />

                <TextView
                    android:id="@+id/tv_your_rate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/grey"
                    android:textSize="@dimen/note1"
                    tools:text="₹ 800/-" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin_color"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_5"
            android:gravity="center"
            android:orientation="horizontal"
            android:visibility="visible" />

    </LinearLayout>

    <ImageView
        android:id="@+id/iv_checked"
        android:layout_width="13dp"
        android:layout_height="20dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_weight="0.1"
        android:background="@drawable/checkedcheckbox_rate"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/iv_unchecked"
        android:layout_width="13dp"
        android:layout_height="20dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_weight="0.1"
        android:background="@drawable/check_ref" />

    <ImageView
        android:id="@+id/iv_locked"
        android:layout_width="13dp"
        android:layout_height="20dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_weight="0.1"
        android:background="@drawable/gray_check"
        android:visibility="gone" />

</LinearLayout>